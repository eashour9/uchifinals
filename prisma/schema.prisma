generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Exam {
  course       String
  section      String
  sectionTitle String
  instructor   String   @default("")
  startTime    String
  endTime      String
  date         DateTime
  room         String
  id           Int      @id @default(autoincrement())

  @@index([course, section])
  @@map("exams")
}
